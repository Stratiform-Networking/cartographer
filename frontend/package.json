    "test:storybook:run": "vitest --project=storybook --run",
    "lint": "eslint . --ext .ts,.vue --max-warnings=300",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,css}\""
  },
  "dependencies": {
    "@clerk/clerk-js": "^5.55.2",
    "axios": "^1.13.5",
    "d3": "^7.9.0",
    "dompurify": "^3.3.1",
    "marked": "^17.0.1",
    "pinia": "^2.2.6",
    "posthog-js": "^1.342.1",
    "vue": "^3.4.38",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-onboarding": "^10.1.4",
    "@storybook/addon-vitest": "^10.1.4",
    "@storybook/vue3-vite": "^10.1.4",
    "@testing-library/vue": "^8.1.0",
    "@types/d3": "^7.4.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^17.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.47",
    "prettier": "^3.7.4",
    "storybook": "^10.1.11",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^4.0.15"
